<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="cm_MyTopBar">
        <attr name="cm_myTopBarTitle" format="string" />
        <attr name="cm_myTopBarLeftIcon" format="reference" />
        <attr name="cm_myTopBarRightIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="cm_MyFormBar">
        <attr name="cm_myFormBarTitle" format="string" />
        <attr name="cm_myFormBarHint" format="string" />
    </declare-styleable>

    <declare-styleable name="cm_MySignalView">
        <attr name="cm_mySignalLight" format="enum">
            <enum name="green" value="0" />
            <enum name="red" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="cm_MyMenuBar">
        <attr name="cm_myMenuIcon" format="reference" />
        <attr name="cm_myMenuText" format="string" />
    </declare-styleable>
</resources>