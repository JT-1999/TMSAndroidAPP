<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="30dp"
        android:left="30dp"
        android:right="0dp"
        android:top="0dp">
        <shape
            android:innerRadius="16dp"
            android:shape="ring"
            android:thickness="4dp"
            android:useLevel="false">
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>
    <item
        android:bottom="55dp"
        android:left="62dp"
        android:right="22dp"
        android:top="15dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="16dp"
                    android:height="30dp" />
                <corners
                    android:bottomLeftRadius="8dp"
                    android:bottomRightRadius="8dp" />
                <solid android:color="@color/cm_color_universal_foreground" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="59dp"
        android:left="70dp"
        android:right="22dp"
        android:top="11dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="8dp"
                    android:height="30dp" />
                <corners
                    android:bottomLeftRadius="4dp"
                    android:bottomRightRadius="4dp" />
                <solid android:color="@color/mm_color_universal_repair" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="65dp"
        android:left="78dp"
        android:right="10dp"
        android:top="5dp">
        <rotate android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <size
                    android:width="12dp"
                    android:height="30dp" />
                <solid android:color="@color/mm_color_universal_repair" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="10dp"
        android:left="25dp"
        android:right="55dp"
        android:top="40dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="18dp"
                    android:height="50dp" />
                <corners
                    android:bottomLeftRadius="9dp"
                    android:bottomRightRadius="9dp" />
                <solid android:color="@color/cm_color_universal_foreground" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="14dp"
        android:left="33dp"
        android:right="57dp"
        android:top="38dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="48dp" />
                <corners
                    android:bottomLeftRadius="5dp"
                    android:bottomRightRadius="5dp" />
                <solid android:color="@color/mm_color_universal_repair" />
            </shape>
        </rotate>
    </item>
</layer-list>