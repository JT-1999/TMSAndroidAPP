<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="60dp"
        android:left="60dp">
        <shape
            android:innerRadius="58dp"
            android:shape="ring"
            android:thickness="10dp"
            android:useLevel="false">
            <solid android:color="@color/mm_color_fragment_mine_contacts" />
        </shape>
    </item>
    <item
        android:bottom="70dp"
        android:left="0dp"
        android:right="70dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="30dp"
                android:height="30dp" />
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>
    <item
        android:bottom="0dp"
        android:left="70dp"
        android:right="0dp"
        android:top="70dp">
        <shape android:shape="rectangle">
            <size
                android:width="30dp"
                android:height="30dp" />
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>
    <item
        android:bottom="55dp"
        android:left="15dp"
        android:right="60dp"
        android:top="20dp">
        <rotate android:fromDegrees="-20">
            <shape android:shape="rectangle">
                <size
                    android:width="25dp"
                    android:height="25dp" />
                <corners android:radius="8dp" />
                <solid android:color="@color/mm_color_fragment_mine_contacts" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="15dp"
        android:left="55dp"
        android:right="20dp"
        android:top="60dp">
        <rotate android:fromDegrees="20">
            <shape android:shape="rectangle">
                <size
                    android:width="25dp"
                    android:height="25dp" />
                <corners android:radius="8dp" />
                <solid android:color="@color/mm_color_fragment_mine_contacts" />
            </shape>
        </rotate>
    </item>
</layer-list>