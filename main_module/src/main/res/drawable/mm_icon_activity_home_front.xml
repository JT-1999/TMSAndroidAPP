<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="20dp"
        android:left="20dp"
        android:right="20dp"
        android:top="20dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="62%"
            android:pivotY="55%">
            <shape android:shape="rectangle">
                <solid android:color="@color/cm_color_universal_app_theme_color" />
                <size
                    android:width="36dp"
                    android:height="36dp" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="50dp">
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="50dp" />
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>
    <item
        android:bottom="15dp"
        android:left="22dp"
        android:right="22dp"
        android:top="50dp">
        <shape android:shape="rectangle">
            <size
                android:width="56dp"
                android:height="35dp" />
            <solid android:color="@color/cm_color_universal_app_theme_color" />
        </shape>
    </item>
    <item
        android:bottom="15dp"
        android:left="42dp"
        android:right="42dp"
        android:top="60dp">
        <shape android:shape="rectangle">
            <size
                android:width="16dp"
                android:height="25dp" />
            <solid android:color="@color/cm_color_universal_foreground" />
        </shape>
    </item>
</layer-list>